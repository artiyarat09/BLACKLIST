<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".page2">


    <ImageButton
        android:id="@+id/search"
        android:layout_width="172dp"
        android:layout_height="169dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/art2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/search"
        android:layout_marginStart="48dp"
        android:text="ค้นหารายชื่อ"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/search"
        android:layout_marginEnd="43dp"
        android:text="บุคคลควรระวัง"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="79dp"
        android:layout_marginEnd="56dp"
        android:text="เกี่ยวกับเรา"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="32dp"
        android:text="รายการทั้งหมด"
        android:textSize="30sp" />

    <ImageButton
        android:id="@+id/about1"
        android:layout_width="172dp"
        android:layout_height="169dp"
        android:layout_above="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="14dp"
        android:background="@drawable/art1" />

    <ImageButton
        android:id="@+id/protect1"
        android:layout_width="172dp"
        android:layout_height="169dp"
        android:layout_alignStart="@+id/about1"
        android:layout_alignTop="@+id/search"
        android:background="@drawable/art4" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="175dp"
        android:layout_height="169dp"
        android:layout_above="@+id/textView4"
        android:layout_alignStart="@+id/search"
        android:background="@drawable/art3" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView4"
        android:layout_marginStart="62dp"
        android:text="วิธีป้องกัน"
        android:textSize="18sp" />

    <FrameLayout
        android:id="@+id/main_nav"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">


    </FrameLayout>



    <android.support.design.widget.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="?android:windowBackground"
        app:menu="@menu/bottom_navigation" >

    </android.support.design.widget.BottomNavigationView>



</RelativeLayout>

